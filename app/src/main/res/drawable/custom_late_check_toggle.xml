<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <bitmap android:src="@drawable/ic_check_circle_green_600" />
    </item>
    <item android:state_checked="true" android:state_pressed="true">
        <bitmap android:src="@drawable/ic_check_circle_green_700" />
    </item>
    <item>
        <bitmap android:src="@drawable/ic_warning_red_600" />
    </item>
    <item android:state_pressed="true">
        <bitmap android:src="@drawable/ic_warning_red_700" />
    </item>
</selector>